{
  "name": "camp-manager",
  "version": "0.2.1-beta",
  "private": false,
  "description": "A camp manager made with Vue.js, Electron & TailwindCSS",
  "author": "Francesco Monti",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "build:dev": "vue-cli-service build --mode development",
    "build:mainproc": "webpack-cli --mode=development --config webpack.mainproc.config.js",
    "clean": "rm -rf ./dist && rm -rf ./bin && rm -rf ./dist.electron",
    "dist": "electron-builder -c electron-builder.yml",
    "pack": "electron-builder --dir -c electron-builder.yml",
    "start": "electron .",
    "test": "npm run test:unit",
    "generate-icons": "vsvg -s ./static/svg/ -t ./src/assets/icons --ext ts"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^4.4.0",
    "@fullcalendar/interaction": "^4.4.0",
    "@fullcalendar/moment": "^4.4.0",
    "@fullcalendar/timegrid": "^4.4.0",
    "@fullcalendar/vue": "^4.4.0",
    "nedb": "^1.8.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.0",
    "vue-resize-directive": "^1.2.0",
    "vue-router": "^3.1.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.9",
    "@types/electron": "^1.6.10",
    "@types/mocha": "^7.0.1",
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-plugin-typescript": "^4.2.2",
    "@vue/cli-plugin-unit-mocha": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "chai": "^4.2.0",
    "electron": "^8.0.1",
    "electron-builder": "^22.4.1",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.7.5",
    "vue-cli-plugin-tailwind": "~1.2.0",
    "vue-svgicon": "^3.2.6",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11"
  },
  "bugs": "https://github.com/Lothindir/camp-manager/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lothindir/camp-manager.git"
  }
}
