{
  "name": "camp-manager",
  "version": "0.2.3-beta",
  "private": false,
  "description": "A camp manager made with Vue.js, Electron & TailwindCSS",
  "author": "Francesco Monti",
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:dev": "vue-cli-service build --mode development",
    "build:mainproc": "webpack-cli --mode=development --config webpack.mainproc.config.js",
    "clean": "rm -rf ./dist && rm -rf ./bin && rm -rf ./dist.electron",
    "dist": "electron-builder -c electron-builder.yml",
    "pack": "electron-builder --dir -c electron-builder.yml",
    "start": "electron .",
    "dev": "nodemon --watch src --ext * --exec \"npm run build:dev && npm run start\"",
    "test": "npm run test:unit",
    "generate-icons": "vsvg -s ./static/svg/ -t ./src/assets/icons --ext ts"
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/moment": "^5.5.0",
    "@fullcalendar/timegrid": "^5.5.0",
    "@fullcalendar/vue": "^5.5.0",
    "@types/fullcalendar__vue": "^4.4.2",
    "nedb": "^1.8.0",
    "typeface-quicksand": "^1.1.13",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-resize-directive": "^1.2.0",
    "vue-router": "^3.4.9"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/electron": "^1.6.10",
    "@types/mocha": "^8.2.0",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-plugin-unit-mocha": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/test-utils": "^1.1.2",
    "chai": "^4.2.0",
    "electron": "^11.1.0",
    "electron-builder": "^22.9.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.6",
    "sass-loader": "^9.0.3",
    "source-map-loader": "^1.1.3",
    "typescript": "^4.1.3",
    "vue-cli-plugin-tailwind": "~1.4.2",
    "vue-template-compiler": "^2.6.12",
    "vue-svgicon": "^3.2.9",
    "webpack-cli": "^3.3.12"
  },
  "bugs": "https://github.com/Lothindir/camp-manager/issues",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lothindir/camp-manager.git"
  }
}
